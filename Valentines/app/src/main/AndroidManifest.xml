<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/valentines_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/valentines_icon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Valentines"
        tools:targetApi="31">
        <activity
            android:name=".StartActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FinalActivity"
            android:exported="false" />
        <activity
            android:name=".BeValentineActivity"
            android:exported="false" />
        <activity
            android:name=".CrushActivity"
            android:exported="false" />
        <activity
            android:name=".AbsolutelyActivity"
            android:exported="false"
            android:parentActivityName=".LikeActivity" />
        <activity
            android:name=".DefinitelyActivity"
            android:exported="false"
            android:parentActivityName=".LikeActivity" />
        <activity
            android:name=".YesActivity"
            android:exported="false"
            android:parentActivityName=".LikeActivity" />
        <activity
            android:name=".LikeActivity"
            android:exported="false"
            android:parentActivityName=".StartActivity"/>

        </application>


</manifest>